@model List<Project>

@{
	ViewData["Title"] = "Index";
}

<div class="container text-center">
    
    <div class="row">
        @foreach (var p in Model)
        {
            <div class="col">
                <p>@p.Id</p>
                <p>@p.Title</p>
                <p>@p.Category</p>
                <p>@p.Description</p>
                <p>@p.TechnologiesUsed</p>
                <p>@p.StartDate</p>
                <p>@p.EndDate</p>
                <p>@p.ProjectUrl</p>
            </div>
        }
    </div>
</div>